<?xml version="1.0"?>
<config>
	  <tabs>
		<product_additional_files translate="label" module="productfileupload">
            <label>Product Additional Files</label>
            <sort_order>450</sort_order>
        </product_additional_files>
	  </tabs>
	  <sections>
		<product_additional_files_general  translate="label" module="productfileupload">                    
		<label>General</label>
		<tab>product_additional_files</tab>
		<frontend_type>text</frontend_type>
		<sort_order>10</sort_order>
		<show_in_default>1</show_in_default>
		<show_in_website>1</show_in_website>
		<show_in_store>1</show_in_store>            
					<groups>
				      <product_additional_files_info translate="label"> 
					  <label>General Information</label>
					  <frontend_type>text</frontend_type>
					  <sort_order>10</sort_order>
					  <show_in_default>1</show_in_default>
					  <show_in_website>1</show_in_website>
					  <show_in_store>1</show_in_store>
				       <fields>
                          <jquery_enable translate="label">
                            <label>Add jQuery Library File</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>20</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
							<comment>Add jQuery library file if jQuery library is not added before in adminhtml.</comment>
                          </jquery_enable>
					   </fields>
					   </product_additional_files_info>
					</groups>
		</product_additional_files_general>
	  </sections>
</config>
